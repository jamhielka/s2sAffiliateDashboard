A,iBAhBA,CAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA,KAzBA;AA0BAK,YA1BA,oBA0BAC,IA1BA,EA0BA;AAEA;AACA;AAEA;AACA,KAhCA;AAkCAC,cAlCA,sBAkCAD,IAlCA,EAkCA;AAEA;AACA;AACAN;AACA;AACA,KAxCA;AA0CAQ,qBA1CA,+BA0CA;AAAA;;AACA;AACAR,gEAFA,CAGA;;AACA,gBACAS,MADA,CACA,oBADA,EACA;AACA9B;AACAuB;AADA;AADA,OADA,EAKA,eALA,EAOAC,IAPA,CAOA;AACAH;;AACA,kCAFA,CAGA;;;AAEA;AACAU;AACA,OAdA,EAeAN,KAfA,CAeA;AACAJ,uBADA,CAEA;AACA,OAlBA;AAmBA;AACA,KAlEA;AAoEAW,SApEA,mBAoEA;AAAA;;AACA;AACA;AACA;AACA;AACA,OAHA;AAIA,KA1EA;AA4EAC,eA5EA,yBA4EA;AAAA;;AACA;AACA;AACA;AACA;AACA,OAHA;AAIA,KAlFA;AAoFAC,QApFA,kBAoFA;AAAA;;AACA;;AACA;AACA,kBACAC,GADA,CACA,oBADA,EACA,eADA,EACA;AACAnC;AACAuB;AADA;AADA,SADA,EAMAC,IANA,CAMA;AACAH;;AACA,oCAFA,CAGA;;;AAEA;AACAU;AACA,SAbA,EAcAN,KAdA,CAcA;AACAJ,yBADA,CAEA;AACA,SAjBA;AAkBAe;AACA,OApBA,MAoBA;AACA,kBACAC,IADA,CACA,oBADA,EACA,eADA,EACA;AACArC;AACAuB;AADA;AADA,SADA,EAMAC,IANA,CAMA;AACAH;;AACA,oCAFA,CAGA;;;AAEA;AACAU;;AACA;AACA,SAdA,EAeAN,KAfA,CAeA;AACAJ,yBADA,CAEA;AACA,SAlBA;AAmBA;;AACA;AACA;AAhIA;AAvDA","names":["data","search","dialog","dialogDelete","headers","text","align","sortable","value","desserts","editedIndex","editedItem","name","code","defaultItem","computed","formTitle","watch","val","created","methods","initialize","getForMerchantList","TToken","localStorage","console","get","Authorization","then","catch","editItem","item","deleteItem","deleteItemConfirm","delete","alert","close","closeDelete","save","put","Object","post"],"sourceRoot":"src/views/pages","sources":["department.vue"],"sourcesContent":["<template>\r\n  <v-data-table\r\n    :search=\"search\"\r\n    :headers=\"headers\"\r\n    :items=\"desserts\"\r\n    sort-by=\"name\"\r\n    class=\"elevation-1\"\r\n  >\r\n    <template v-slot:top>\r\n      <v-toolbar flat>\r\n        <v-toolbar-title>Department</v-toolbar-title>\r\n        <v-divider class=\"mx-4\" inset vertical></v-divider>\r\n        <v-spacer></v-spacer>\r\n        <v-text-field\r\n          v-model=\"search\"\r\n          append-icon=\"mdi-magnify\"\r\n          label=\"Search\"\r\n          single-line\r\n          hide-details\r\n        ></v-text-field>\r\n        <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-btn color=\"primary\" dark class=\"mb-2\" v-bind=\"attrs\" v-on=\"on\">\r\n              New Item\r\n            </v-btn>\r\n          </template>\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"text-h5\">{{ formTitle }}</span>\r\n            </v-card-title>\r\n\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\" >\r\n                    <v-text-field\r\n                      v-model=\"editedItem.name\"\r\n                      label=\"Name\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" >\r\n                    <v-text-field\r\n                      v-model=\"editedItem.code\"\r\n                      label=\"Code\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"close\"> Cancel </v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"save\"> Save </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n        <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n          <v-card>\r\n            <v-card-title class=\"text-h5\"\r\n              >Are you sure you want to delete this item?</v-card-title\r\n            >\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"closeDelete\"\r\n                >Cancel</v-btn\r\n              >\r\n              <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\"\r\n                >OK</v-btn\r\n              >\r\n              <v-spacer></v-spacer>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n    </template>\r\n    <template v-slot:[`item.actions`]=\"{ item }\">\r\n      <v-icon small class=\"mr-2\" @click=\"editItem(item)\"> mdi-pencil </v-icon>\r\n      <v-icon small @click=\"deleteItem(item)\"> mdi-delete </v-icon>\r\n    </template>\r\n    <template v-slot:no-data>\r\n      <v-btn color=\"primary\" @click=\"initialize\"> Reset </v-btn>\r\n    </template>\r\n  </v-data-table>\r\n</template>\r\n<script>\r\nexport default {\r\n  data: () => ({\r\n     search: \"\",\r\n    dialog: false,\r\n    dialogDelete: false,\r\n    headers: [\r\n      {\r\n        text: \"Department ID\",\r\n        align: \"start\",\r\n        sortable: true,\r\n        value: \"_id\",\r\n      },\r\n      {\r\n        text: \"Department Name\",\r\n        align: \"start\",\r\n        sortable: false,\r\n        value: \"name\",\r\n      },\r\n      { text: \"Code\", value: \"code\" },\r\n      { text: \"Date/Time\", value: \"createdDateTimeLabel\" },\r\n      { text: \"Actions\", value: \"actions\", sortable: false },\r\n    ],\r\n    desserts: [],\r\n    editedIndex: -1,\r\n    editedItem: {\r\n    \r\n      name: \"\",\r\n      code: \"\",\r\n    },\r\n    defaultItem: {\r\n    \r\n      name: \"\",\r\n      code: \"\",\r\n    },\r\n  }),\r\n\r\n  computed: {\r\n    formTitle() {\r\n      return this.editedIndex === -1 ? \"New Item\" : \"Edit Item\";\r\n    },\r\n  },\r\n\r\n  watch: {\r\n    dialog(val) {\r\n      val || this.close();\r\n    },\r\n    dialogDelete(val) {\r\n      val || this.closeDelete();\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.initialize();\r\n  },\r\n\r\n  methods: {\r\n    initialize() {\r\n      this.getForMerchantList();\r\n    },\r\n    async getForMerchantList() {\r\n      // this.table.loading = true;\r\n      var TToken = localStorage.getItem(\"token\");\r\n      console.log(TToken);\r\n      await this.$api\r\n        .get(\"admin/departments\", {\r\n          headers: {\r\n            Authorization: TToken,\r\n          },\r\n        })\r\n\r\n        .then((response) => {\r\n          console.log(response);\r\n          const newArr = response.data.data.departments;\r\n          this.desserts = newArr;\r\n          //this.table.loading = false;\r\n        })\r\n        .catch((e) => {\r\n          //this.table.loading = false;\r\n          console.log(e);\r\n        });\r\n    },\r\n    editItem(item) {\r\n     \r\n      this.editedIndex = this.desserts.indexOf(item);\r\n      this.editedItem = Object.assign({}, item);\r\n\r\n      this.dialog = true;\r\n    },\r\n\r\n    deleteItem(item) {\r\n    \r\n      this.editedIndex = this.desserts.indexOf(item);\r\n      this.editedItem = Object.assign({}, item);\r\n      console.log(this.editedItem);\r\n      this.dialogDelete = true;\r\n    },\r\n\r\n    deleteItemConfirm() {\r\n      var TToken = localStorage.getItem(\"token\");\r\n   console.log(\"deleteItemConfirm \"+ this.editedItem._item);\r\n      // this.desserts.splice(this.editedIndex, 1);\r\n      this.$api\r\n        .delete(\"/admin/departments\", {\r\n          headers: {\r\n            Authorization: TToken,\r\n          }},\r\n          this.editedItem\r\n        )\r\n        .then((response) => {\r\n          console.log(response);\r\n          this.$emit(\"obj\", true);\r\n          // this.loadingBtn = false;\r\n\r\n          this.dialog = false;\r\n          alert(\"Department is successfully deleted\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n          //this.loadingBtn = false;\r\n        });\r\n      this.closeDelete();\r\n    },\r\n\r\n    close() {\r\n      this.dialog = false;\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem);\r\n        this.editedIndex = -1;\r\n      });\r\n    },\r\n\r\n    closeDelete() {\r\n      this.dialogDelete = false;\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem);\r\n        this.editedIndex = -1;\r\n      });\r\n    },\r\n\r\n    save() {\r\n      var TToken = localStorage.getItem(\"token\");\r\n      if (this.editedIndex > -1) {\r\n        this.$api\r\n          .put(\"/admin/departments\", this.editedItem, {\r\n            headers: {\r\n              Authorization: TToken,\r\n            },\r\n          })\r\n          .then((response) => {\r\n            console.log(response);\r\n            this.$emit(\"obj\", true);\r\n            // this.loadingBtn = false;\r\n\r\n            this.dialog = false;\r\n            alert(\"Department is successfully edited\");\r\n          })\r\n          .catch((e) => {\r\n            console.log(e);\r\n            //this.loadingBtn = false;\r\n          });\r\n        Object.assign(this.desserts[this.editedIndex], this.editedItem);\r\n      } else {\r\n        this.$api\r\n          .post(\"/admin/departments\", this.editedItem, {\r\n            headers: {\r\n              Authorization: TToken,\r\n            },\r\n          })\r\n          .then((response) => {\r\n            console.log(response);\r\n            this.$emit(\"obj\", true);\r\n            // this.loadingBtn = false;\r\n\r\n            this.dialog = false;\r\n            alert(\"Department is successfully created\");\r\n            this.initialize()\r\n          })\r\n          .catch((e) => {\r\n            console.log(e);\r\n            //this.loadingBtn = false;\r\n          });\r\n      }\r\n      this.close();\r\n    },\r\n  },\r\n};\r\n</script>"]}]}